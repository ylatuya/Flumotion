<components>
  <component type="http-streamer">
    <source location="http.py"/>
    <properties>
      <property name="source" type="string" required="yes"/>
      <property name="port" type="int" required="yes"/>
      <property name="logfile" type="string"/>
      <property name="gst-property" type="xml" multiple="yes"/>
      <property name="auth" type="string"/>
      <property name="maxclients" type="int"/>
    </properties>
  </component>
</components>
