<registry>
  <components>
  
    <component type="smoke-encoder">
      <source location="flumotion.component.encoders.smoke.smoke"/>
      <eater name="default"/>
      <feeder name="default"/>
      <properties>
        <property name="feed" type="string" multiple="yes"/>
        <property name="source" type="string" multiple="no"/>
        <property name="qmin" type="int"/>
        <property name="qmax" type="int"/>
        <property name="threshold" type="int"/>
        <property name="keyframe" type="int"/>
      </properties>
    </component>
  
    <component type="speex-encoder">
      <source location="flumotion.component.encoders.speex.speex"/>
      <eater name="default"/>
      <feeder name="default"/>
      <properties>
        <property name="feed" type="string" multiple="yes"/>
        <property name="source" type="string" multiple="no"/>
        <property name="bitrate" type="int"/>
      </properties>
    </component>
  
    <component type="mulaw-encoder">
      <source location="flumotion.component.encoders.mulaw.mulaw"/>
      <eater name="default"/>
      <feeder name="default"/>
      <properties>
        <property name="feed" type="string" multiple="yes"/>
        <property name="source" type="string" multiple="no"/>
      </properties>
    </component>
  
  </components>
</registry> 
