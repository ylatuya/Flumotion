<components>
  <component type="theora-encoder">
    <source location="flumotion.component.encoders.theora"/>
    <properties>
      <property name="source" type="string" multiple="no"/>
      <property name="bitrate" type="int"/>
      <property name="quality" type="int"/>
    </properties>
  </component>
  <component type="smoke-encoder">
    <source location="flumotion.component.encoders.smoke"/>
