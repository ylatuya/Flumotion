<registry>
  <components>
  
    <component type="theora-encoder">
      <source location="flumotion.component.encoders.theora.theora"/>
      <eater name="default"/>
      <feeder name="default"/>
      <properties>
        <property name="feed" type="string" multiple="yes"/>
        <property name="source" type="string" multiple="no"/>
        <property name="bitrate" type="int"/>
        <property name="quality" type="int"/>
        <property name="keyframe-threshold" type="int" />
        <property name="keyframe-mindistance" type="int" />
        <property name="keyframe-maxdistance" type="int" />
        <property name="quick-compress" type="int" />
        <property name="noise-sensitivity" type="int" />
      </properties>
    </component>
  
    <component type="smoke-encoder">
      <source location="flumotion.component.encoders.smoke.smoke"/>
      <eater name="default"/>
      <feeder name="default"/>
      <properties>
        <property name="feed" type="string" multiple="yes"/>
        <property name="source" type="string" multiple="no"/>
        <property name="qmin" type="int"/>
        <property name="qmax" type="int"/>
        <property name="threshold" type="int"/>
        <property name="keyframe" type="int"/>
      </properties>
    </component>
  
    <component type="jpeg-encoder">
      <source location="flumotion.component.encoders.jpeg.jpeg"/>
      <eater name="default"/>
      <feeder name="default"/>
      <properties>
        <property name="feed" type="string" multiple="yes"/>
        <property name="source" type="string" multiple="no"/>
        <property name="quality" type="int"/>
        <property name="framerate" type="float"/>
      </properties>
    </component>
  
    <component type="vorbis-encoder">
      <source location="flumotion.component.encoders.vorbis.vorbis"/>
      <eater name="default"/>
      <feeder name="default"/>
      <properties>
        <property name="feed" type="string" multiple="yes"/>
        <property name="source" type="string" multiple="no"/>
        <property name="bitrate" type="int"/>
	<property name="quality" type="float"/>
	<property name="channels" type="int"/>
      </properties>
    </component>
  
    <component type="speex-encoder">
      <source location="flumotion.component.encoders.speex.speex"/>
      <eater name="default"/>
      <feeder name="default"/>
      <properties>
        <property name="feed" type="string" multiple="yes"/>
        <property name="source" type="string" multiple="no"/>
        <property name="bitrate" type="int"/>
      </properties>
    </component>
  
    <component type="mulaw-encoder">
      <source location="flumotion.component.encoders.mulaw.mulaw"/>
      <eater name="default"/>
      <feeder name="default"/>
      <properties>
        <property name="feed" type="string" multiple="yes"/>
        <property name="source" type="string" multiple="no"/>
      </properties>
    </component>
  
  </components>
</registry> 
